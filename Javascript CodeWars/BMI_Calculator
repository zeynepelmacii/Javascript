//In this exercise, we are calculating the Body Mass Index (BMI).

function bmi(weight, height) {
  let bmi = weight / (height**2); // We have performed the calculation
  
  if(bmi < 18.5){           // If BMI is less than 18.5, return "Underweight"
    return "Underweight";
  }else if (bmi < 25){      // If BMI is between 18.5 and 25, return "Normal"
    return "Normal";
  }else if (bmi < 30){      // If BMI is between 25 and 30, return "Overweight"
    return "Overweight";
  }else{
    return "Obese";         // If BMI is greater than 30, return "Obese"
  }
}
